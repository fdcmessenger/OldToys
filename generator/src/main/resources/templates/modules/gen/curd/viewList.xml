<?xml version="1.0" encoding="utf-8"?>
<template>
    <name>viewList</name>
    <filePath>src/main/resources/META-INF/resources/views/modules/${moduleName}/${subModuleName}</filePath>
    <fileName>${className}List.jsp</fileName>
    <content><![CDATA[
<%@page contentType="text/html" pageEncoding="UTF-8"%>
<%@ include file="/views/include/taglib.jsp"%>
﻿<!DOCTYPE HTML>
<html lang="zh-cn">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
	<title>${functionNameSimple}管理</title>
        <%@ include file="/views/include/css.jsp"%>
    </head>
    <body>
        <div id="main">
            <div class="hidden-xs" id="${ClassName}TableToolbar" role="group">
                <button type="button" class="btn btn-primary " onclick="${ClassName}.openAdd${ClassName}()">
                    <i class="fa fa-plus"></i>&nbsp;添加
                </button>
                <button type="button" class="btn btn-primary button-margin" onclick="${ClassName}.openEdit${ClassName}()">
                    <i class="fa fa-edit"></i>&nbsp;修改
                </button>
                <button type="button" class="btn btn-primary button-margin" onclick="${ClassName}.delete()" >
                    <i class="fa fa-delete"></i>&nbsp;删除
                </button>
            </div>
            <table id="${ClassName}Table" ></table>
        </div>
        <%@ include file="/views/include/script.jsp"%>
        <script src="${r"${ctxStatic}"}/modules/${moduleName}/${className}List.js"></script>
    </body>
</html>
   ]]>
    </content>
</template>